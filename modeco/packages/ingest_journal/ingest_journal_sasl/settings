#!/usr/bin/env bash

# START CONFIGURATION
export CONTECO_DC_GLOBAL_PACKAGE_CONTECO="journalbeat.sasl"
export CONTECO_DC_GLOBAL_PACKAGE_TAG="7.8.0"
export CONTECO_DC_GLOBAL_STACKNAMES="ingest_journal_sasl"
