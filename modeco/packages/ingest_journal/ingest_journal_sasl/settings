export INGEST_JOURNAL_SASL_ENVIRONMENT_OUTPUT_ELASTICSEARCH="false"
export INGEST_JOURNAL_SASL_ENVIRONMENT_OUTPUT_KAFKA="false"
export INGEST_JOURNAL_SASL_ENVIRONMENT_OUTPUT_LOGSTASH="false"
export INGEST_JOURNAL_SASL_ENVIRONMENT_OUTPUT_CONFIG_ELASTICSEARCH_HOSTS="elasticsearch:9200"
export INGEST_JOURNAL_SASL_ENVIRONMENT_OUTPUT_CONFIG_ELASTICSEARCH_INDEX="ingest_journal"
export INGEST_JOURNAL_SASL_ENVIRONMENT_OUTPUT_CONFIG_ELASTICSEARCH_SSL="false"
export INGEST_JOURNAL_SASL_ENVIRONMENT_OUTPUT_CONFIG_ELASTICSEARCH_USERNAME="elasticsearch"
export INGEST_JOURNAL_SASL_ENVIRONMENT_OUTPUT_CONFIG_ELASTICSEARCH_PASSWORD="password"
export INGEST_JOURNAL_SASL_ENVIRONMENT_OUTPUT_CONFIG_LOGSTASH_HOSTS="logstash:5044"
export INGEST_JOURNAL_SASL_ENVIRONMENT_OUTPUT_CONFIG_LOGSTASH_INDEX="ingest_journal"
export INGEST_JOURNAL_SASL_ENVIRONMENT_OUTPUT_CONFIG_KIBANA_HOST="kibana:5601"
export INGEST_JOURNAL_SASL_ENVIRONMENT_OUTPUT_CONFIG_KIBANA_DASHBOARDS="false"
export INGEST_JOURNAL_SASL_ENVIRONMENT_OUTPUT_CONFIG_KAFKA_HOSTS="kafka:9092"
export INGEST_JOURNAL_SASL_ENVIRONMENT_OUTPUT_CONFIG_KAFKA_SSL="false"
export INGEST_JOURNAL_SASL_ENVIRONMENT_OUTPUT_CONFIG_KAFKA_USERNAME="kafka"
export INGEST_JOURNAL_SASL_ENVIRONMENT_OUTPUT_CONFIG_KAFKA_PASSWORD="password"
export INGEST_JOURNAL_SASL_ENVIRONMENT_OUTPUT_CONFIG_KAFKA_TOPIC="ingest_journal"
export INGEST_JOURNAL_SASL_LABELS=
# END OTHER VARIABLES

# START VOLUMES VARIABLES
# END VOLUMES VARIABLES

# START PORTS VARIABLES
# END PORTS VARIABLES

# START NETWORKS VARIABLES
# END NETWORKS VARIABLES

# START DEPLOY VARIABLES
export INGEST_JOURNAL_SASL_DEPLOY_LABELS=
export INGEST_JOURNAL_SASL_DEPLOY_REPLICAS=1
export INGEST_JOURNAL_SASL_DEPLOY_MODE="replicated"
if [[ "${INGEST_JOURNAL_SASL_DEPLOY_MODE}" == "replicated" ]] ; then export INGEST_JOURNAL_SASL_DEPLOY_MODE_REPLICATED_REPLICAS="replicas: ${INGEST_JOURNAL_SASL_DEPLOY_REPLICAS}"; else export export INGEST_JOURNAL_SASL_DEPLOY_MODE_REPLICATED_REPLICAS= ; fi
export INGEST_JOURNAL_SASL_DEPLOY_PLACEMENT_CONSTRAINTS=
export INGEST_JOURNAL_SASL_DEPLOY_RESOURCES_LIMITS_CPUS=
