export INGEST_JOURNAL_SASL_DEPLOY_RESOURCES_LIMITS_MEMORY="128m"
export INGEST_JOURNAL_SASL_DEPLOY_RESOURCES_RESERVATIONS_MEMORY="8m"
# END DEPLOY VARIABLES
