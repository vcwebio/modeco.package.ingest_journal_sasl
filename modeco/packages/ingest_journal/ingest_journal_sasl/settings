export CONTECO_DC_GLOBAL_RUNSTACKS="ingest_journal_sasl"
export CONTECO_DC_GLOBAL_VOLUME_LIST="package_ingest_journal_sasl_journalbeat_volume"
export CONTECO_DC_GLOBAL_VOLUME_INITIALISED=""
# END CONFIGURATION

# START OTHER VARIABLES
export INGEST_JOURNAL_SASL_ENVIRONMENT_FILEPATHS="/hostfs/var/log/journal"
export INGEST_JOURNAL_SASL_ENVIRONMENT_FILEPATHS_MAPPING=",'/var/log/journal:/hostfs/var/log/journal:ro'"
